[
  {{- $pages := where .Site.Pages "Type" "in" (slice "post") -}}
  {{- range $index, $page := $pages -}}
  {
    "id": {{ $index }},
    "title": {{ .Title | jsonify }},
    "url": {{ .RelPermalink | jsonify }},
    "content": {{ .Plain | jsonify }}
  }{{ if ne (add $index 1) (len $pages) }},{{ end }}
  {{- end -}}
]
